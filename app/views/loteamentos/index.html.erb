<p id="notice"><%= notice %></p>

<h1>Loteamentos</h1>

<table>
  <thead>
    <tr>
      <th>Longitude</th>
      <th>Latitude</th>
      <th>Terreno</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @loteamentos.each do |loteamento| %>
      <tr>
        <td><%= loteamento.longitude %></td>
        <td><%= loteamento.latitude %></td>
        <td><%= loteamento.terreno_id %></td>
        <td><%= link_to 'Show', loteamento %></td>
        <td><%= link_to 'Edit', edit_loteamento_path(loteamento) %></td>
        <td><%= link_to 'Destroy', loteamento, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Loteamento', new_loteamento_path %>
